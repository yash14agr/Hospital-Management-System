        <label for="number">Phone Number:</label>
		<input type="number" placeholder="Enter Number" name="number" class="input1"><br>
        <label for="city">City:</label>
		<input type="text" placeholder="city" name="city" class="input1"><br>
        <label for="symtoms">Specialization:</label>
